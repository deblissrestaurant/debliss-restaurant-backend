#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/HP/Documents/DeBliss Project/Zenith2.0/node_modules/.pnpm/@dotenvx+dotenvx@1.47.5/node_modules/@dotenvx/dotenvx/src/cli/node_modules:/mnt/c/Users/HP/Documents/DeBliss Project/Zenith2.0/node_modules/.pnpm/@dotenvx+dotenvx@1.47.5/node_modules/@dotenvx/dotenvx/src/node_modules:/mnt/c/Users/HP/Documents/DeBliss Project/Zenith2.0/node_modules/.pnpm/@dotenvx+dotenvx@1.47.5/node_modules/@dotenvx/dotenvx/node_modules:/mnt/c/Users/HP/Documents/DeBliss Project/Zenith2.0/node_modules/.pnpm/@dotenvx+dotenvx@1.47.5/node_modules/@dotenvx/node_modules:/mnt/c/Users/HP/Documents/DeBliss Project/Zenith2.0/node_modules/.pnpm/@dotenvx+dotenvx@1.47.5/node_modules:/mnt/c/Users/HP/Documents/DeBliss Project/Zenith2.0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/HP/Documents/DeBliss Project/Zenith2.0/node_modules/.pnpm/@dotenvx+dotenvx@1.47.5/node_modules/@dotenvx/dotenvx/src/cli/node_modules:/mnt/c/Users/HP/Documents/DeBliss Project/Zenith2.0/node_modules/.pnpm/@dotenvx+dotenvx@1.47.5/node_modules/@dotenvx/dotenvx/src/node_modules:/mnt/c/Users/HP/Documents/DeBliss Project/Zenith2.0/node_modules/.pnpm/@dotenvx+dotenvx@1.47.5/node_modules/@dotenvx/dotenvx/node_modules:/mnt/c/Users/HP/Documents/DeBliss Project/Zenith2.0/node_modules/.pnpm/@dotenvx+dotenvx@1.47.5/node_modules/@dotenvx/node_modules:/mnt/c/Users/HP/Documents/DeBliss Project/Zenith2.0/node_modules/.pnpm/@dotenvx+dotenvx@1.47.5/node_modules:/mnt/c/Users/HP/Documents/DeBliss Project/Zenith2.0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@dotenvx/dotenvx/src/cli/dotenvx.js" "$@"
else
  exec node  "$basedir/../@dotenvx/dotenvx/src/cli/dotenvx.js" "$@"
fi
